{
  "name": "react-isomorfic-sample",
  "version": "0.0.1",
  "description": "sample for react isomorfic app",
  "main": "index.js",
  "scripts": {
    "test": "run-p -c lint:ts type-check",
    "lint": "run-p -c lint:ts-*",
    "lint:ts": "tslint --project tsconfig.json --config tslint.prod.json 'src/**/*.ts{,x}' --exclude '**/*.{spec,story,d}.ts{,x}'",
    "lint:ts-test": "tslint --project tsconfig.json --config tslint.test.json 'src/**/*.{spec,story}.ts{,x}' --exclude '**/*.d.ts'",
    "fix": "yarn lint:ts --fix && yarn lint:ts-test --fix",
    "precommit": "yarn type-check",
    "commit": "git-cz",
    "start": "cross-env nodemon scripts/dev.ts --config nodemon.json --",
    "type-check": "tsc"
  },
  "keywords": [
    "react",
    "isomorfic"
  ],
  "author": "Alex Werman",
  "license": "ISC",
  "resolutions": {
    "babel-core": "^7.0.0-bridge.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-proposal-decorators": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/runtime-corejs2": "^7.2.0",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/helmet": "^0.0.42",
    "@types/lodash": "^4.14.119",
    "@types/react": "^16.7.17",
    "@types/react-dom": "^16.0.11",
    "@types/rimraf": "^2.0.2",
    "@types/webpack": "^4.4.21",
    "@types/webpack-dev-middleware": "^2.0.2",
    "@types/webpack-hot-middleware": "^2.16.4",
    "@types/webpack-manifest-plugin": "^2.0.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-webpack-alias": "^2.1.2",
    "commitizen": "^3.0.5",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "fork-ts-checker-webpack-plugin": "^0.5.2",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "rimraf": "^2.6.2",
    "tslint": "^5.12.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.2",
    "webpack": "^4.27.1",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.3"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-manifest-helpers": "^0.5.0",
    "helmet": "^3.15.0",
    "lodash": "^4.17.11",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-hot-loader": "^4.6.3",
    "webpack-manifest-plugin": "^2.0.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
