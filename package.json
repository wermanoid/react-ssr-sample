    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-manifest-helpers": "^0.6.0",
    "helmet": "^3.16.0",
    "jss": "^9.8.7",
    "lodash": "^4.17.11",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.8.4",
    "react-jss": "^8.6.1",
    "webpack-manifest-plugin": "^2.0.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
