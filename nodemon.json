{
  "restartable": "rs",
  "ignore": [".git", "node_modules"],
  "verbose": true,
  "execMap": {
    "ts": "babel-node --extensions '.js,.jsx,.ts,.tsx'",
    "js": "babel-node --extensions '.js,.jsx,.ts,.tsx'"
  },
  "watch": ["src/server", "scripts", "tsconfig.json", "config/webpack"],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js json ts tsx"
}
