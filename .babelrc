{
  "presets": [
    "@babel/typescript",
    "@babel/env",
    "@babel/react",
    "@babel/stage-0"
  ],
  "plugins": [
    "lodash",
    ["babel-plugin-styled-components", { "ssr": true }],
    [ "babel-plugin-webpack-alias", { "config": "./webpack.config.resolve.js" } ],
    "dynamic-import-webpack"
  ],
  "env": {
    "test": {
      "presets": [
        "@babel/typescript",
        "@babel/env",
        "@babel/react",
        "@babel/stage-0"
      ],
      "plugins":[
        ["babel-plugin-styled-components", { "ssr": true }],
        [ "babel-plugin-webpack-alias", { "config": "./webpack.config.resolve.js" } ],
      ],
    }
  }
}
